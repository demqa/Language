	

	.SECTION text
	
_start:
	call main
	ret
	
;;--------------------------

	
FUCKtorial:
	push rbp
	mov rbp, rsp
	

	
;; empty mark IF number 1
	mov rax, [rbp+16]
	mov rcx, rax
	mov rax, 1
	mov rdx, rax
	cmp rcx, rdx
	ja if_end_1
	

	mov rax, 1
	pop rbp
	ret
	
if_end_1:
	mov rax, [rbp+16]
	mov rcx, rax
	sub rsp, 0
	
	push rcx
	push rdx
	

	mov rax, [rbp+16]
	mov rcx, rax
	mov rax, 1
	mov rdx, rax
	mov rax, rcx
	sub rax, rdx
	

	push rax
	

	call FUCKtorial
	

	pop rdx
	

	pop rdx
	pop rcx
	
	add rsp, 0
	

	mov rdx, rax
	mov rax, rcx
	mul rdx
	

	pop rbp
	ret
	;; no main for now
