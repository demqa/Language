FUCK:
	push rbp
	mov rbp, rsp
	

	mov rax, [rbp-16]
	mov rcx, rax
	mov rax, [rbp-24]
	mov rdx, rax
	mov rax, rcx
	add rax, rdx
	

	mov [rbp+8], rax
	

	mov rax, [rbp-16]
	mov rcx, rax
	mov rax, 1
	mov rdx, rax
	mov rax, rcx
	mul rdx
	

	mov rcx, rax
	mov rax, 2
	mov rdx, rax
	mov rax, rcx
	mul rdx
	

	mov rcx, rax
	mov rax, [rbp-32]
	mov rdx, rax
	mov rax, rcx
	mul rdx
	

	pop rbp
	ret
	

	
baza:
	push rbp
	mov rbp, rsp
	

	mov rax, 20
	pop rbp
	ret
	

	;; no main for now
